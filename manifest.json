{
  "manifest_version": 2,
  "name": "Extension FroggedTV",
  "version": "1.0",
  "description": "Extension pour savoir quand la FroggedTV est en stream.",
  "permissions": [
    "notifications",
    "tabs",
    "management",
    "storage"
  ],
  "commands": {
    "open-froggedtv-site": {
      "suggested_key": {
        "default": "Ctrl+Shift+O",
        "mac": "Command+Shift+O"
      },
      "description": "Toggle feature foo"
    }
  },
  "browser_action": {
    "default_icon" : "img/ftv-green.png",
    "default_popup": "index.html"
  },
  "background": {
      "scripts": ["jquery.js", "background.js"],
      "persistent": false
  }
}